@model CS4790A3.Models.Runners

<tr class="runner">
    @using (Html.BeginForm("addRunner","Home"))
    {
        @Html.AntiForgeryToken()


        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.HiddenFor(model => model.contactID)

        <td>
            @Html.EditorFor(model => model.firstName, new { htmlAttributes = new { @class = "form-control first" } })
            @Html.ValidationMessageFor(model => model.firstName, "", new { @class = "text-danger" })
        </td>
        <td>
            @Html.EditorFor(model => model.lastName, new { htmlAttributes = new { @class = "form-control last" } })
            @Html.ValidationMessageFor(model => model.lastName, "", new { @class = "text-danger" })
        </td>
        <td>
            @Html.EditorFor(model => model.tShirt, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.tShirt, "", new { @class = "text-danger" })
        </td>
    }
</tr>




